{
	"servers": {
		"svelte-llm": {
			"command": "npx",
			"args": ["mcp-remote", "https://svelte-llm.stanislav.garden/mcp/mcp"]
		},
		"context7": {
			"type": "http",
			"url": "https://mcp.context7.com/mcp",
			"headers": {
				"CONTEXT7_API_KEY": "YOUR_API_KEY"
			}
		},
		"github-mcp": {
			"type": "http",
			"url": "https://api.githubcopilot.com/mcp"
		}
	}
}
