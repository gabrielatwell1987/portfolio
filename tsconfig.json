{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "strict": true,
        "moduleResolution": "bundler",
        "lib": [
            "DOM",
            "DOM.Iterable"
        ],
        "types": [
            "svelte",
            "vite/client",
            "@types/node"
        ]
    },
    "include": [
        "src/**/*",
    ],
    "exclude": [
        "node_modules",
        ".svelte-kit"
    ]
}