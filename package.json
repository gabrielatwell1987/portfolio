{
	"name": "portfolio",
	"version": "1.0.0",
	"private": true,
	"engines": {
		"node": ">=22.0.0",
		"npm": ">=10.0.0"
	},
	"scripts": {
		"dev": "vite dev --host --open",
		"prebuild": "node -e \"const fs = require('fs'); const version = Date.now().toString(); fs.writeFileSync('static/version.json', JSON.stringify({version}, null, 2));\"",
		"build": "vite build",
		"preview": "vite preview --host",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test": "vitest",
		"check": "svelte-check"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/adapter-vercel": "^5.10.2",
		"@sveltejs/kit": "^2.42.2",
		"@sveltejs/vite-plugin-svelte": "^6.1.0",
		"@vite-pwa/assets-generator": "^0.2.4",
		"dotenv": "^16.4.5",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.45.1",
		"gsap": "^3.12.5",
		"mdsvex": "^0.11.2",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte-check": "^4.0.4",
		"three": "^0.167.1",
